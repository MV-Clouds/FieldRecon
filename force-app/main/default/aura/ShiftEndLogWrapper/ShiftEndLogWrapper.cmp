<aura:component implements="flexipage:availableForAllPageTypes,force:lightningQuickActionWithoutHeader" controller="ShiftEndLogController" access="global" >

    <aura:attribute name="thirdLevelAccess" type="Boolean" default="false" />   
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:if isTrue="{!v.thirdLevelAccess}">
        <div style = "width : 100%;">
            <c:shiftEndLog onclosebox='{!c.handleCloseEvent}' />
        </div>
        <aura:set attribute="else">
            <c:libMessageWithIllustration title="Feature Inaccessible" message="Third Level Feature Pack Required! Please Contact Your Admin!" /> 
        </aura:set>
    </aura:if>
</aura:component>